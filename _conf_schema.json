{
  "mcsm_url": {
    "description": "MCSManager 面板地址(必填)‼️",
    "type": "string",
    "hint": "例如: http://127.0.0.1:23333,不要带/",
    "obvious_hint": true
  },
  "api_key": {
    "description": "API Key(必填)‼️",
    "type": "string",
    "hint": "使用管理员API Key，非管理员没测，不会获取的看readme.md",
    "obvious_hint": true
  },
  "authorized_users": {
    "description": "授权用户列表（uid）",
    "type": "list",
    "hint": "不配置时默认所有人有权限，配置后只有列表中的用户可以使用（白名单模式）",
    "default": []
  },
  "authorized_groups": {
    "description": "授权群组列表",
    "type": "list",
    "hint": "不配置时默认所有人有权限，配置后只有列表中的群组可以使用（白名单模式）",
    "default": []
  },
  "filtered_nodes": {
    "description": "要排除的节点UUID列表",
    "type": "list",
    "hint": "在使用mcsm list指令时排除这些节点",
    "default": []
  },
  "log_size": {
    "description": "日志获取行数",
    "type": "int",
    "hint": "使用mcsm log指令时返回的日志条数，默认20条，建议100以内",
    "default": 20
  }
}